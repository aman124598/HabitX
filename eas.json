{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "samsung-store": {
      "distribution": "store",
      "env": { "NODE_VERSION": "20.19.4" },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "autoIncrement": true,
      "developmentClient": false
    },
    "production-apk": {
      "distribution": "store",
      "env": { "NODE_VERSION": "20.19.4" },
      "android": {
        "buildType": "apk"
      },
      "autoIncrement": true,
      "developmentClient": false
    },
    "production-aab": {
      "distribution": "store",
      "env": { "NODE_VERSION": "20.19.4" },
      "android": {
        "buildType": "app-bundle"
      },
      "autoIncrement": true
    },
    "production": {
      "distribution": "store",
      "env": { "NODE_VERSION": "20.19.4" },
      "android": {}
    },
    "preview": {
      "distribution": "internal",
      "env": { "NODE_VERSION": "20.19.4" }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": { "NODE_VERSION": "20.19.4" }
    }
  },
  "submit": {
    "production-aab": {
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json",
        "track": "production"
      }
    }
  }
}
